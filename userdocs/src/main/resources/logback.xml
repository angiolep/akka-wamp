<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} |-%-5level in %logger{8} - %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="akka.wamp" level="DEBUG">
    <appender-ref ref="STDOUT"/>
  </logger>

  <root level="DEBUG" />
</configuration>